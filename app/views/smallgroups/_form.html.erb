<%= simple_form_for(@smallgroup) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :descriptin %>
    <%= f.input :image %>
    <%= f.input :address %>
    <%= f.input :postcode %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
